// Simple Class, No Inheritance.

class Mew {
	u32 PublicField;
	string AnotherPublicField;

	// Private fields should start with two underscores.
	isize __PrivateField;
	string __AnotherPrivateField;

	func Mew() {
		// Constructor

		this.__AnotherPrivateField = "Mew";
	}

	func return_cry() {
		// Access to class' method

		return this.__AnotherPrivateField
	}
}

func main() {
	Mew my_mew = new Mew();  // new mew hehe

	string cry = my_mew.return_cry();
}
